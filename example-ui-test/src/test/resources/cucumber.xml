<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"

	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"

	xsi:schemaLocation="
		http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-4.1.xsd
		http://www.springframework.org/schema/context
		http://www.springframework.org/schema/context/spring-context-4.1.xsd
		">

	<import resource="bdd_config.xml" />

	<!-- Beans for Page Objects -->
	<bean id="mainPg" class="com.fmi.pageobject.ExampleMainPageObj">
		<constructor-arg type="com.fmi.bdd.Driver" ref="driverService" />
	</bean>

	<bean id="childPg" class="com.fmi.pageobject.ExampleChildPageObj">
		<constructor-arg type="com.fmi.bdd.Driver" ref="driverService" />
	</bean>

</beans>
